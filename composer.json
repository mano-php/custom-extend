{
    "name": "mano-code/custom-extend",
    "alias": "ManoCode中间层",
    "description": "基于owl-admin和amis的底层自定义拓展",
    "type": "library",
    "version": "v1.2.1",
    "keywords": ["extension"],
    "homepage": "https://github.com/mano-php/custom-extend",
    "license": "MIT",
    "authors": [
        {
            "name": "Mano",
            "email": "mano@qq.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "slowlyo/owl-admin": "*",
        "slowlyo/owl-dict": "*"
    },
    "autoload": {
        "psr-4": {
            "ManoCode\\CustomExtend\\": "src/"
        },
        "files": [
            "./src/Functions.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "ManoCode\\CustomExtend\\CustomExtendServiceProvider"
            ]
        }
    }
}
